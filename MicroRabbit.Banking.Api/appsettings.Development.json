{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.Seq" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj} <s:{SourceContext}>{NewLine}{Exception}"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:30060",
          "apiKey": ""
        }
      }
    ],
    "Properties": {
      "ApplicationName": "MicroRabbit.Banking.Api"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
    }
  },
  "ConnectionStrings": {
    "BankingDbContext": "User ID=guest;Password=guest;Host=localhost;Port=50010;Database=MicroRabbit;Pooling=true;",
    "RabbitMQ": "amqp://guest:guest@localhost:50020"
  },
  "RabbitMQ": {
    "Hostname": "localhost",
    "Port": 50020,
    "Username": "guest",
    "Password": "guest"
  },
  "SeqSettings": {
    "ServerUrl": "http://localhost:30060/ingest/otlp/v1/logs",
    "HeaderName": "X-Seq-ApiKey",
    "SeqApiKey": ""
  },
  "ApplicationName": "MicroRabbit.Banking.Api"
}
